net: "train_test_stage_1.prototxt"
test_iter: 100
test_interval: 1000

solver_type: SGD
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 100000
momentum: 0.9
weight_decay: 0.0005

display: 100
max_iter: 400000

snapshot: 1000
snapshot_prefix: "snapshots/VGG_PReLU_Roads_Mini"

solver_mode: GPU
device_id: 0

random_seed: 1701

