name: "VGG_PReLU_FCN_Roads_Mini"
layer {
  name: "input_data"
  type: "Data"
  top: "input_data"
  data_param {
    backend: LMDB
    source: "../../data/mass_roads_mini/lmdb/train_sat"
    batch_size: 16
  }
  include: { phase: TRAIN }
}
