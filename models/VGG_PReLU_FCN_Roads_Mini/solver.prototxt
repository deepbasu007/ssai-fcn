net: "train_test.prototxt"
test_iter: 100
test_interval: 1000

solver_type: SGD
base_lr: 1e-5
lr_policy: "step"
gamma: 0.001
stepsize: 3000
momentum: 0.9
weight_decay: 0.0005

display: 100
max_iter: 400000

snapshot: 10000
snapshot_prefix: "snapshots/VGG_PReLU_FCN_Roads_Mini"

solver_mode: GPU
device_id: 0

random_seed: 1701

